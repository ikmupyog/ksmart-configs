{
  "key": "consolidatedreceipt",
  "config": {
    "defaultStyle": {
      "font": "Cambay"
    },
    "content": [
      {
        "style": "ws-head",
        "table": {
          "widths": [
            "*"
          ],
          "body": [
            [
              {
                "stack": [
                  {
                    "text": "Receipt",
                    "style": "receipt-main-header"
                  },
                  {
                    "image": "{{logotenant}}",
                    "width": 70,
                    "height": 70,
                    "alignment": "centre",
                    "style": "receipt-logo-header"
                  },
                  {
                    "image": "{{qr_code_image}}",
                    "width": 70,
                    "height": 70,
                    "style": "qr-code"
                  },
                  {
                    "text": "{{tenantEn}}",
                    "style": "receipt-header"
                  },
                  {
                    "text": "Type Of Service : Civil Registration",
                    "style": "receipt-service-header"
                  }
                ]
              }
            ]
          ]
        },
        "layout": "noBorders"
      },
      {
        "style": "payment-table",
        "fillColor": "#eeeeee",
        "table": {
          "widths": [500,500],
          "body": [
            [
            {
              "text": "{{receipt_no}}: {{receiptNo}}                                                            Date :{{receiptDate}} ",
              "style": "receipt-table-value",
              "border": [
                true,
                true,
                true,
                false
              ]
            }
            ],
            [
              {
                "fillColor": "#eeeeee",
                "margin": [
                  10,30,0,30
                ],
                "border": [
                  true,
                  false,
                  true,
                  true
                ],
                "table": {
                  "widths": [30,230,90,95],
                  "body": [
                    [
                      "Sl No",
                      "Payer ID",
                      "{{payer_name}}",
                      "{{payer_contact}}"
                    ],
                    [
                      "{{slNo}}",
                      "{{consumerID}}",
                      "{{payerName}}",
                      "{{payerContact}}"
                    ]
                  ]
                },
                "layout": {
                  "hLineColor": "#858181",
                  "vLineColor": "#858181"
                }
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "next-payment-table",
        "fillColor": "#eeeeee",
        "table": {
          "widths": [500,500],
          "body": [
            [
              {
                "text": "{{payment_mode}}: Online          Instrument: Digital           {{transaction_id}}: {{transactionID}} ",
                "style": "receipt-table-value",
                "border": [
                  true,
                  true,
                  true,
                  false
                ]
              }
            ],
            [
              {
                "fillColor": "#eeeeee",
                "margin": [
                  10,30,10,30
                ],
                "border": [
                  true,
                  false,
                  true,
                  false
                ],
                "table": {
                  "widths": [30,90,90,170,55],
                  "body": [
                    [
                      "Sl No",
                      "Head Code",
                      "Head Name",
                      "Financial Year",
                      "Amount"
                    ],
                    [
                      "{{slNo}}",
                      "{{headCode}}",
                      "{{headName}}",
                      "{{financialYear}}",
                      "{{dueAmount}}"
                    ]
                  ]
                },
               "layout": {
                 "hLineColor": "#858181",
                 "vLineColor": "#858181"
            }
              }
            ],
            [
              {
                "text": "{{total_bill_amount}}:  {{dueAmount}}                 {{paid_amount}}: {{paidAmount}}                 Balance/Advance: {{pendingAmount}} ",
                "style": "receipt-table-value",
                "border": [
                  true,
                  false,
                  true,
                  true
                ]
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "bottom-payment-table",
        "fillColor": "#fceae8",
        "table": {
          "widths": [500,500],
          "body": [
            [
              {
                "text": "Generated By:  {{user-name}}                                                          LB Secretary: {{commissioner/eo}}",
                "style": "receipt-bottom-value",
                "border-width": "thin",
                "border": [
                  true,
                  true,
                  true,
                  true
                ]
              }
            ]
          ]
        },
        "layout": {}
      },
      {
        "style": "no-signature",
        "text": "{{no_signature}}"
      }
    ],
    "styles": {

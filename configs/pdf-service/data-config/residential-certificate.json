{
  "key": "residential-certificate",
  "DataConfigs": {
    "serviceName": "rainmaker-common",
    "version": "1.0.0",
    "baseKeyPath": "$.ResidetialCertificate.*",
    "entityIdPath": "$.id",
    "isCommonTableBorderRequired": true,
    "mappings": [
      {
        "topic": "common-pdf-generation-3",
        "mappings": [
          {
            "direct": [
              {
                "variable": "logoImage",
                "url": "https://raw.githubusercontent.com/egovernments/egov-web-app/master/web/rainmaker/dev-packages/egov-ui-kit-dev/src/assets/images/pblogo.png",
                "type": "image"
              },
              {
                "variable": "name",
                "value": {
                  "path": "$.name"
                },
                "type": "setEmpty"
              },
              {
                "variable": "address",
                "value": {
                  "path": "$.address"
                },
                "type": "setEmpty"
              },
              {
                "variable": "lbName",
                "value": {
                  "path": "$.lbName"
                },
                "type": "setEmpty"
              },
                {
                "variable": "tenantName",
                "value": {
                  "path": "$.tenantId"
                },
                "localisation": {
                  "required": true,
                  "prefix": "TENANT_TENANTS",
                  "module": "rainmaker-common"
                }
              },
              {
                "variable": "lbAddressWithPinCode",
                "value": {
                  "path": "$.lbAddressWithPinCode"
                },
                "type": "setEmpty"
              }
            ]
          }
        ]
      }
    ]
  }
}
